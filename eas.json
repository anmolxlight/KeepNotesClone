{
  "cli": {
    "version": ">= 0.52.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "env": {
        "APP_ENV": "development"
      },
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "env": {
        "APP_ENV": "preview"
      },
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
